{{>layout/header}}

<h1>게시글 등록</h1>

<div class="col-md-12">
    <form>
        <div class="form-group">
            <label for="msg"> 메세지 내용 </label>
            <input type="text" class="form-control" id="msg" placeholder="제목을 입력하세요">
        </div>
        <div class="form-group">
            <label for="author"> 전화번호 </label>
            <input type="text" class="form-control" id="phoneNumber" placeholder="작성자를 입력하세요">
        </div>
    </form>
    <a href="/" role="button" class="btn btn-secondary">취소</a>
    <button type="button" class="btn btn-primary" id="btn-save">등록</button>
</div>

{{>layout/footer}}